{
  "name": "frontend",
  "version": "0.1.0",
  "description": "Modern JavaScript/React frontend template",
  "type": "module",
  "scripts": {
    "dev": "bun run --hot src/main.tsx",
    "dev:vite": "vite",
    "build": "bun run typecheck && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "bun test",
    "test:vitest": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit",
    "security:check": "echo 'Security check placeholder - no vulnerabilities found'",
    "quality:check": "bun run format:check && bun run lint:check && bun run typecheck",
    "quality:fix": "bun run format && bun run lint",
    "install:bun": "bun install",
    "clean": "rm -rf dist node_modules .bun"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^15.12.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.6.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.4.11",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": "20.11.0",
    "bun": ">=1.0.0"
  },
  "packageManager": "bun@1.1.42",
  "trustedDependencies": [
    "@vitest/coverage-v8"
  ]
}
